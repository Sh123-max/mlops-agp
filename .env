# PostgreSQL / MLflow
POSTGRES_USER=mlflow
POSTGRES_PASSWORD=mlflow123
POSTGRES_DB=mlflow_db
POSTGRES_PORT=5432

# MLflow now runs on port 5001
MLFLOW_BACKEND_STORE_URI=postgresql://mlflow:mlflow123@postgres:5432/mlflow_db
MLFLOW_ARTIFACT_ROOT=/mlflow/artifacts
MLFLOW_TRACKING_URI=http://localhost:5001

# Monitoring
PUSHGATEWAY_URL=http://pushgateway:9091

# Jenkins webhook token
JENKINS_WEBHOOK_TOKEN=my-jenkins-webhook-token

# Project
PROJECT_NAME=diabetes

# Resource limits (optimized for 8 GB)
OMP_NUM_THREADS=2
MKL_NUM_THREADS=2
OPENBLAS_NUM_THREADS=2
NUM_WORKER_THREADS=3
